    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended",
            "prettier"
        ],
        "env": {
            "browser": true,
            "node": true,
            "es6": true
        },
        "root": true,
        "parser": "babel-eslint",
        "parserOptions": {
            "ecmaVersion": 8,
            "ecmaFeatures": {
                "legacyDecorators": true,
                "jsx": true
            },
            "sourceType": "module"
        },
        "plugins": [
            "babel",
            "react"
        ],
        "rules": {
            "max-len": [
                2,
                {
                    "code": 120,
                    "tabWidth": 2,
                    "ignoreUrls": true
                }
            ],
            "template-curly-spacing": "off",
            "indent": "off",
            "semi": [
                "error",
                "always"
            ],
            "no-trailing-spaces": [
                "error",
                {
                    "skipBlankLines": true
                }
            ],
            "no-unused-vars": 1,
            "react/prop-types": [
                "off",
                {
                    "ignore": [
                        "dispatch"
                    ],
                    "skipUndeclared": true
                }
            ]
        }
    },
    "devDependencies": {
        "eslint-config-next": "^11.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "prettier": "^2.3.2"
    }
}